{
  "name": "realities-api",
  "version": "0.0.1",
  "description": "A backend for tribal decentralised organisations.",
  "engines": {
    "npm": "5.5.1",
    "node": "8.9.1"
  },
  "scripts": {
    "start": "concurrently --kill-others \"webpack --watch\" \"nodemon build/bundle.js\"",
    "test": "mocha",
    "build": "better-npm-run build",
    "serve": "better-npm-run serve"
  },
  "betterScripts": {
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "serve": {
      "command": "node build/bundle.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "apollo-server-express": "^1.4.0",
    "axios": "^0.18.0",
    "better-npm-run": "^0.1.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "eslint": "^4.13.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "graphql": "^14.0.2",
    "graphql-resolvers": "^0.2.2",
    "graphql-tools": "^2.24.0",
    "jwks-rsa": "^1.2.1",
    "lodash": "^4.17.11",
    "mime": "^2.0.3",
    "mocha": "^4.0.1",
    "neo4j-driver": "^1.6.3",
    "node-cron": "^2.0.3",
    "nodemon": "^1.12.4",
    "normalize-url": "^4.0.0",
    "path": "^0.12.7",
    "source-map-support": "^0.5.0",
    "uuid": "^3.3.2",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
